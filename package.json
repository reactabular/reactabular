{
  "private": true,
  "name": "reactabular",
  "version": "1.0.0",
  "description": "A framework for building the React table you need",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "init": "npm install && lerna bootstrap",
    "start": "catalog start docs",
    "test": "jest --",
    "test:coverage": "jest --coverage --",
    "test:watch": "jest --watch --",
    "test:lint": "eslint . --ext .js --ext .jsx --cache",
    "gh-pages:build-and-deploy": "npm run gh-pages:build && npm run gh-pages:deploy",
    "gh-pages:build": "catalog build docs",
    "gh-pages:deploy": "gh-pages -d docs/build",
    "publish": "lerna publish",
    "postinstall": "node lib/post_install.js",
    "postpublish": "npm run gh-pages:build-and-deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactabular/reactabular.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "table",
    "tables"
  ],
  "files": [
    "lib/post_install.js",
    "lerna.json",
    "packages/**/*.js"
  ],
  "author": "Juho Vepsalainen <bebraw@gmail.com> (http://github.com/bebraw)",
  "license": "MIT",
  "main": "packages/reactabular",
  "bugs": {
    "url": "https://github.com/reactabular/reactabular/issues"
  },
  "homepage": "http://reactabular.js.org/",
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ],
    "setupFiles": [
      "./lib/test_setup.js"
    ]
  },
  "devDependencies": {
    "annogenerate": "^0.8.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "catalog": "^3.0.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "deep-diff": "^0.3.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gh-pages": "^0.12.0",
    "git-prepush-hook": "^1.0.2",
    "jest": "^19.0.2",
    "lerna": "2.0.0-beta.37",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "purecss": "^0.6.2",
    "react": "^16.0.0",
    "react-dnd": "^2.5.3",
    "react-dnd-html5-backend": "^2.5.3",
    "react-dom": "^16.0.0",
    "react-edit": "^6.4.0",
    "react-github-corner": "^1.0.0",
    "react-pagify": "^2.4.0",
    "react-redux": "^5.0.2",
    "react-test-renderer": "^16.0.0",
    "react-visibility-toggles": "^1.2.0",
    "reactabular-column-extensions": "^8.9.3",
    "reactabular-easy": "^9.2.0",
    "reactabular-resizable": "^8.9.0",
    "redux": "^3.6.0",
    "schema2object": "^0.4.0",
    "searchtabular": "^1.7.0",
    "segmentize": "^0.4.1",
    "selectabular": "^2.1.0",
    "sortabular": "^1.5.0",
    "styled-components": "^2.2.0",
    "stylesheet-helpers": "^0.2.2",
    "table-resolver": "^3.2.0",
    "treetabular": "^3.4.0",
    "uuid": "^3.0.1",
    "webpack": "^3.6.0"
  },
  "pre-push": [
    "test:lint",
    "test"
  ]
}
